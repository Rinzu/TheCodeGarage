# Python Port Scanning Script

# cmdline is a function to run External Commands from Python Script
from subprocess import PIPE, Popen


def cmdline(command):
    process = Popen(
        args=command,
        stdout=PIPE,
        shell=True
    )
    return process.communicate()[0]

print "Your IP Address: "
print cmdline("ipconfig getifaddr en0")

print "Your router IP Address: "
print cmdline("ipconfig getifaddr en1")

